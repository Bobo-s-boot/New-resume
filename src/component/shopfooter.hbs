<footer>
<div class="row">
  {{#each footer}}
     <div class="col-2">
      <ul class="nav flex-column">
       {{#each this}}
        <li>
          <a class="nav-link {{#if @last}} disabled {{/if}}" href="{{this.link}}">{{this.text}}</a>
        </li>
       {{/each}}
      </ul>
     </div>
  {{/each}}
</div>
</footer>