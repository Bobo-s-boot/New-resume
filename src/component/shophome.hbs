{{#with service}}
  <div class='card text-bg-success'>
    <div class='card-header text-center'>
      <h1>{{title}}</h1>
    </div>
    <div class='card-body text-center'>
      <p>{{description}}</p>
      {{#each buttons}}
        <button
          class='btn btn-dark {{link}}'
        >{{text}}</button>
      {{/each}}
    </div>
  </div>
{{/with}}