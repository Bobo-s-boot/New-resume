<div>
    {{#with main}}
    <main>
        <section>
            {{> title data='My educationss' big=true}}
            <ul class="ps-5">
                {{#each educations}}
                <li>{{name}}
                    {{#unless isend}} ===(ще в процесі) {{/unless }}
                </li>
                {{/each}}

            </ul>
        </section>
        <section>
            {{> title data="My certificats" }}
        <ul class="ps-5">
            {{#each certificates}}
            <li>{{name}}
                <strong>- ID сертифікату ({{id}})</strong>
            </li>
            {{/each}}
         </ul>
        </section>
    </main>

    {{/with}}
    </div>