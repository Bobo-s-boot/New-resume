<div>
    {{#with main}}
    <main>
        <section>
            {{> title data='My educations' big=true}}
            <ul>
                {{#each educations}}
                <li>{{name}}
                    {{#unless isend}} ===(ще в процесі) {{/unless }}
                </li>
                {{/each}}

            </ul>
        </section>
        <section>
            {{> title data="My certificats" }}
            {{#each certificates}}
            <li>{{name}}
                <strong>- ID сертифікату ({{id}})</strong>
            </li>
            {{/each}}
        </section>
    </main>

    {{/with}}
</div>